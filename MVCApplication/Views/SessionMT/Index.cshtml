<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <div>
        <h2>@ViewBag.MyProp</h2>
        <h2>@ViewBag.MyProp1</h2>
        <h2>@ViewBag.MyProp2</h2>

    </div>
    <div>
        <ul>
            @foreach (string item in ViewBag.MyList)
            {
                <li>@item</li>
            }
        </ul>
    </div>
    <div>
        <ul>
            @foreach (var item in ViewBag.MyModelList)
            {
                <li>@item.Name</li>
                <li>@item.age</li>
                <li>@item.email</li>


            }
        </ul>
    </div>
</body>
</html>
